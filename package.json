{
  "name": "with-vercel-website",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "ci": "payload migrate && pnpm build",
    "clear": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/clear.ts",
    "clear:force": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/clear.ts --force",
    "clear:all": "pnpm clear:applications && pnpm clear:sections && pnpm clear:steps && pnpm clear:albums && pnpm clear:films && pnpm clear:books && pnpm clear:art && pnpm clear:media",
    "clear:art": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/clear.ts art --force",
    "clear:books": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/clear.ts books --force",
    "clear:films": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/clear.ts films --force",
    "clear:albums": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/clear.ts albums --force",
    "clear:media": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/clear.ts media --force",
    "clear:applications": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/clear.ts applications --force",
    "clear:sections": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/clear.ts sections --force",
    "clear:steps": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/clear.ts steps --force",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev -p 3001",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "import:all": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/import.ts --all",
    "import:art": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/import.ts --art",
    "import:books": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/import.ts --books",
    "import:films": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/import.ts --films",
    "import:albums": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/import.ts --albums",
    "import:steps": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/import-steps.ts --basic",
    "import:steps:short": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/import-steps.ts --short-text",
    "import:steps:long": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/import-steps.ts --long-text",
    "import:steps:multiple": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/import-steps.ts --multiple-choice",
    "import:steps:advanced": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/import-steps-advanced.ts --all",
    "import:steps:rating": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/import-steps-advanced.ts --rating",
    "import:steps:this-or-that": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/import-steps-advanced.ts --this-or-that",
    "import:steps:ranking": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/import-steps-advanced.ts --ranking",
    "import:statements": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/import-statements.ts",
    "import:applications": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/import-applications.ts",
    "seed:all": "pnpm import:art && pnpm import:books && pnpm import:films && pnpm import:albums && pnpm import:steps && pnpm import:statements && pnpm import:steps:advanced && pnpm import:applications",
    "validate:quiz-assets": "node scripts/js/validate_quiz_assets.js",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=playwright.config.ts",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.0",
    "@payloadcms/admin-bar": "3.59.1",
    "@payloadcms/db-postgres": "3.59.1",
    "@payloadcms/live-preview-react": "3.59.1",
    "@payloadcms/next": "3.59.1",
    "@payloadcms/payload-cloud": "3.59.1",
    "@payloadcms/plugin-form-builder": "3.59.1",
    "@payloadcms/plugin-nested-docs": "3.59.1",
    "@payloadcms/plugin-redirects": "3.59.1",
    "@payloadcms/plugin-search": "3.59.1",
    "@payloadcms/plugin-seo": "3.59.1",
    "@payloadcms/richtext-lexical": "3.59.1",
    "@payloadcms/storage-s3": "3.59.1",
    "@payloadcms/ui": "3.59.1",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "geist": "^1.3.0",
    "graphql": "^16.8.2",
    "lucide-react": "^0.378.0",
    "next": "15.4.4",
    "next-sitemap": "^4.2.3",
    "payload": "3.59.1",
    "prism-react-renderer": "^2.3.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "7.45.4",
    "sharp": "0.34.2",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@playwright/test": "1.54.1",
    "@tailwindcss/typography": "^0.5.13",
    "@testing-library/react": "16.3.0",
    "@types/escape-html": "^1.0.2",
    "@types/node": "22.5.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.5.2",
    "autoprefixer": "^10.4.19",
    "copyfiles": "^2.4.1",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.4.4",
    "jsdom": "26.1.0",
    "playwright": "1.54.1",
    "playwright-core": "1.54.1",
    "postcss": "^8.4.38",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.20.6",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "packageManager": "pnpm@10.18.1",
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
